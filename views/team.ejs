<section class="team_section layout_padding">
  <div class="container">
    <div class="heading_container heading_center">
      <h2>
        <%= pageData.team.title %>
      </h2>
      <p>
       <%= pageData.team.description %>
      </p>
    </div>
    <div class="row">
      <% pageData.team.teamMembers.forEach((team, index) => { %>
      <div class="col-md-4 col-sm-6 mx-auto">
        <div class="box">
          <div class="img-box">
            <img src="<%= team.images %>" alt="">
          </div>
          <div class="detail-box">
            <h5>
              <%= team.name %>
            </h5>
            <h6 class="">
              <%= team.designation %>
            </h6>
          </div>
        </div>
      </div>
    <% }); %>
    </div>
  </div>
</section>
